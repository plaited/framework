{
  "name": "@plaited/cli",
  "version": "0.0.0",
  "engines": {
    "node": ">= v18.12.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/plaited/plaited.git"
  },
  "homepage": "https://github.com/plaited/plaited/blob/main/packages/cli/README.md",
  "type": "module",
  "main": "./dist/lib/index.js",
  "exports": {
    "./app": "./dist/app/index.js",
    ".": "./dist/lib/index.js"
  },
  "module": "./dist/lib/index.js",
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "build:watch": "tsc --watch --project tsconfig.json",
    "prepublishOnly": "yarn build",
    "test": "exit 0"
  },
  "types": "dist/lib/index.d.ts",
  "typesVersions": {
    "*": {
      "app": [
        "./dist/app/index.d.ts"
      ]
    }
  },
  "devDependencies": {
    "@tsconfig/node18": "1.0.1",
    "ava": "5.1.0",
    "ts-node": "10.9.1",
    "typescript": "4.8.4"
  }
}
