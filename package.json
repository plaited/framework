{
  "name": "@plaited/plaited",
  "version": "0.0.4",
  "engines": {
    "node": ">= 14.2.0",
    "npm": ">= 6.14"
  },
  "main": "src/index.js",
  "module": "src/index.js",
  "files": [
    "src/"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "lint": "eslint . --quiet --fix --ext .js",
    "lint:ci": "eslint .  --ext .js",
    "lint:fix": "npm run lint -- --fix",
    "prepare": "husky install",
    "test": "web-test-runner \"tests/**/*.spec.js\" --config web-dev-server.config.js --node-resolve",
    "test:ci": "npm test"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run lint && npm run test:ci && format-package -w"
    }
  },
  "types": "src/index.d.ts",
  "type": "module",
  "dependencies": {
    "@plaited/utils": "^1.1.1",
    "@plaited/behavioral": "^1.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/eslint-parser": "^7.13.10",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-private-methods": "^7.13.0",
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@plaited/assert": "^1.1.1",
    "@web/dev-server-esbuild": "^0.2.11",
    "@web/test-runner": "^0.12.15",
    "cz-conventional-changelog": "^3.3.0",
    "esbuild": "0.9.7",
    "eslint": "^7.21.0",
    "eslint-plugin-compat": "^3.9.0",
    "eslint-plugin-mocha": "^8.1.0",
    "esm": "^3.2.25",
    "format-package": "^6.1.0",
    "husky": "^5.1.3",
    "sinon": "^9.2.4"
  }
}
